{"ast":null,"code":"import { reactive } from 'vue';\nexport default {\n  name: 'App',\n  setup: function () {\n    let person = reactive({\n      name: 'Morries_Wu',\n      age: 18,\n      job: {\n        type: '后端工程师',\n        salary: '30k',\n        a: {\n          b: {\n            c: 666\n          }\n        }\n      },\n      hobby: ['抽烟', '喝酒', '洗澡']\n    });\n\n    function changeInfo() {\n      person.name = '老六';\n      person.age = 20;\n      person.job.type = '算法工程师';\n      person.job.salary = '50k';\n      person.job.a.b.c = 999;\n      person.hobby[0] = '老六';\n      console.log(person.job);\n    }\n\n    function addSex() {\n      person.sex = '男';\n    }\n\n    function deleteName() {\n      delete person.name;\n    }\n    /*返回一个对象(常用)*/\n\n\n    return {\n      person,\n      changeInfo,\n      addSex,\n      deleteName\n    };\n  }\n};","map":{"version":3,"mappings":"AAoBA,SAAQA,QAAR,QAAuB,KAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,KAAK,EAAE,YAAY;IAEjB,IAAIC,MAAK,GAAIH,QAAQ,CAAC;MACpBC,IAAI,EAAE,YADc;MAEpBG,GAAG,EAAE,EAFe;MAGpBC,GAAG,EAAE;QACHC,IAAI,EAAE,OADH;QAEHC,MAAM,EAAE,KAFL;QAGHC,CAAC,EAAE;UACDC,CAAC,EAAE;YACDC,CAAC,EAAE;UADF;QADF;MAHA,CAHe;MAYpBC,KAAK,EAAE,CAAC,IAAD,EAAO,IAAP,EAAa,IAAb;IAZa,CAAD,CAArB;;IAeA,SAASC,UAAT,GAAsB;MACpBT,MAAM,CAACF,IAAP,GAAc,IAAd;MACAE,MAAM,CAACC,GAAP,GAAa,EAAb;MACAD,MAAM,CAACE,GAAP,CAAWC,IAAX,GAAkB,OAAlB;MACAH,MAAM,CAACE,GAAP,CAAWE,MAAX,GAAoB,KAApB;MACAJ,MAAM,CAACE,GAAP,CAAWG,CAAX,CAAaC,CAAb,CAAeC,CAAf,GAAmB,GAAnB;MACAP,MAAM,CAACQ,KAAP,CAAa,CAAb,IAAkB,IAAlB;MACAE,OAAO,CAACC,GAAR,CAAYX,MAAM,CAACE,GAAnB;IACF;;IAEA,SAASU,MAAT,GAAkB;MAChBZ,MAAM,CAACa,GAAP,GAAa,GAAb;IACF;;IAEA,SAASC,UAAT,GAAsB;MACpB,OAAOd,MAAM,CAACF,IAAd;IACF;IAEA;;;IACA,OAAO;MACLE,MADK;MAELS,UAFK;MAGLG,MAHK;MAILE;IAJK,CAAP;EAOF;AA7Ca,CAAf","names":["reactive","name","setup","person","age","job","type","salary","a","b","c","hobby","changeInfo","console","log","addSex","sex","deleteName"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\App.vue"],"sourcesContent":["<template>\n  <div align=\"center\">\n    <h1>一个人的信息</h1>\n    <h2 v-show=\"person.name\">姓名:{{ person.name }}</h2>\n    <h2>年龄:{{ person.age }}</h2>\n    <h2 v-show=\"person.sex\">性别:{{ person.sex }}</h2>\n    <h2>工作种类:{{ person.job.type }}</h2>\n    <h2>工作薪水:{{ person.job.salary }}</h2>\n    <h3>爱好:{{ person.hobby }}</h3>\n    <h3>测试的数据c:{{ person.job.a.b.c }}</h3>\n    <button @click=\"changeInfo\">修改人的信息</button>\n    <button @click=\"addSex\">添加一个sex属性</button>\n    <button @click=\"deleteName\">删除一个name属性</button>\n\n  </div>\n\n\n</template>\n\n<script>\nimport {reactive} from 'vue'\n\nexport default {\n  name: 'App',\n  setup: function () {\n\n    let person = reactive({\n      name: 'Morries_Wu',\n      age: 18,\n      job: {\n        type: '后端工程师',\n        salary: '30k',\n        a: {\n          b: {\n            c: 666\n          }\n        }\n      },\n      hobby: ['抽烟', '喝酒', '洗澡']\n    })\n\n    function changeInfo() {\n      person.name = '老六'\n      person.age = 20\n      person.job.type = '算法工程师'\n      person.job.salary = '50k'\n      person.job.a.b.c = 999\n      person.hobby[0] = '老六'\n      console.log(person.job)\n    }\n\n    function addSex() {\n      person.sex = '男'\n    }\n\n    function deleteName() {\n      delete person.name\n    }\n\n    /*返回一个对象(常用)*/\n    return {\n      person,\n      changeInfo,\n      addSex,\n      deleteName\n    }\n\n  }\n}\n</script>\n\n\n"]},"metadata":{},"sourceType":"module"}