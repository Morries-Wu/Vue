{"ast":null,"code":"import { ref, reactive } from 'vue';\nexport default {\n  name: 'App',\n  setup: function () {\n    /*数据*/\n    let name = ref('Morries_Wu');\n    let age = ref(18);\n    let job = reactive({\n      type: '后端工程师',\n      salary: '30k',\n      a: {\n        b: {\n          c: 666\n        }\n      }\n    });\n    let hobby = [];\n\n    function changeInfo() {\n      name.value = '老六';\n      age.value = 20;\n      job.type = '算法工程师';\n      job.salary = '50k';\n      job.a.b.c = 999;\n      console.log(job);\n    }\n    /*返回一个对象(常用)*/\n\n\n    return {\n      name,\n      age,\n      job,\n      changeInfo\n    };\n  }\n};","map":{"version":3,"mappings":"AAeA,SAAQA,GAAR,EAAaC,QAAb,QAA4B,KAA5B;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,KAAK,EAAE,YAAY;IACjB;IACA,IAAID,IAAG,GAAIF,GAAG,CAAC,YAAD,CAAd;IACA,IAAII,GAAE,GAAIJ,GAAG,CAAC,EAAD,CAAb;IAEA,IAAIK,GAAE,GAAIJ,QAAQ,CAAC;MACjBK,IAAI,EAAE,OADW;MAEjBC,MAAM,EAAE,KAFS;MAGjBC,CAAC,EAAE;QACDC,CAAC,EAAE;UACDC,CAAC,EAAE;QADF;MADF;IAHc,CAAD,CAAlB;IAUA,IAAIC,KAAK,GAAC,EAAV;;IAGA,SAASC,UAAT,GAAsB;MACpBV,IAAI,CAACW,KAAL,GAAa,IAAb;MACAT,GAAG,CAACS,KAAJ,GAAY,EAAZ;MACAR,GAAG,CAACC,IAAJ,GAAW,OAAX;MACAD,GAAG,CAACE,MAAJ,GAAa,KAAb;MACAF,GAAG,CAACG,CAAJ,CAAMC,CAAN,CAAQC,CAAR,GAAY,GAAZ;MACAI,OAAO,CAACC,GAAR,CAAYV,GAAZ;IACF;IAGA;;;IACA,OAAO;MACLH,IADK;MAELE,GAFK;MAGLC,GAHK;MAILO;IAJK,CAAP;EAOF;AAtCa,CAAf","names":["ref","reactive","name","setup","age","job","type","salary","a","b","c","hobby","changeInfo","value","console","log"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\App.vue"],"sourcesContent":["<template>\n  <div align=\"center\">\n    <h1>一个人的信息</h1>\n    <h2>姓名:{{ name }}</h2>\n    <h2>年龄:{{ age }}</h2>\n    <h2>工作种类:{{ job.type }}</h2>\n    <h2>工作薪水:{{ job.salary }}</h2>\n    <h3>测试的数据c:{{ job.a.b.c }}</h3>\n    <button @click=\"changeInfo\">修改人的信息</button>\n  </div>\n\n\n</template>\n\n<script>\nimport {ref, reactive} from 'vue'\n\nexport default {\n  name: 'App',\n  setup: function () {\n    /*数据*/\n    let name = ref('Morries_Wu')\n    let age = ref(18)\n\n    let job = reactive({\n      type: '后端工程师',\n      salary: '30k',\n      a: {\n        b: {\n          c: 666\n        }\n      }\n    })\n\n    let hobby=[]\n\n\n    function changeInfo() {\n      name.value = '老六'\n      age.value = 20\n      job.type = '算法工程师'\n      job.salary = '50k'\n      job.a.b.c = 999\n      console.log(job)\n    }\n\n\n    /*返回一个对象(常用)*/\n    return {\n      name,\n      age,\n      job,\n      changeInfo\n    }\n\n  }\n}\n</script>\n\n\n"]},"metadata":{},"sourceType":"module"}