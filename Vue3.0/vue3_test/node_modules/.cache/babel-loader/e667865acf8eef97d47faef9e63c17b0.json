{"ast":null,"code":"import { customRef } from 'vue';\nexport default {\n  name: 'App',\n  setup: function () {\n    /*使用Vue提供的ref*/\n\n    /*let keyWord = ref('hello')*/\n\n    /*自定义一个ref——名为:myRef*/\n    function myRef(value) {\n      console.log('--myRef', value);\n      return customRef((track, trigger) => {\n        return {\n          get: function () {\n            console.log(`有人把myRef这个容器读取数据了,我把${value}给他了`);\n            /*通知Vue追踪value的*/\n\n            track();\n            return value;\n          },\n          set: function (newValue) {\n            console.log(`有人把myRef这个容器读取数据改为了,${newValue}`);\n            value = newValue;\n            /*通知Vue去重新解析模板*/\n\n            trigger();\n          }\n        };\n      });\n    }\n    /*使用程序员自定义的ref*/\n\n\n    let keyWord = myRef('hello');\n    return {\n      keyWord,\n      myRef\n    };\n  }\n};","map":{"version":3,"mappings":"AAQA,SAAQA,SAAR,QAAwB,KAAxB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,KAAK,EAAE,YAAY;IACjB;;IACA;;IAEA;IACA,SAASC,KAAT,CAAeC,KAAf,EAAsB;MACpBC,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBF,KAAvB;MACA,OAAOJ,SAAS,CAAC,CAACO,KAAD,EAAQC,OAAR,KAAoB;QACnC,OAAO;UACLC,GAAG,EAAE,YAAY;YACfJ,OAAO,CAACC,GAAR,CAAa,uBAAsBF,KAAM,KAAzC;YACA;;YACAG,KAAK;YACL,OAAOH,KAAP;UACD,CANI;UAOLM,GAAG,EAAE,UAAUC,QAAV,EAAoB;YACvBN,OAAO,CAACC,GAAR,CAAa,uBAAsBK,QAAS,EAA5C;YACAP,KAAI,GAAIO,QAAR;YACA;;YACAH,OAAO;UACT;QAZK,CAAP;MAeD,CAhBe,CAAhB;IAiBF;IAEA;;;IACA,IAAII,OAAM,GAAIT,KAAK,CAAC,OAAD,CAAnB;IAEA,OAAO;MACLS,OADK;MAELT;IAFK,CAAP;EAIF;AAnCa,CAAf","names":["customRef","name","setup","myRef","value","console","log","track","trigger","get","set","newValue","keyWord"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\App.vue"],"sourcesContent":["<template>\n  <div align=\"center\">\n    <input type=\"text\" v-model=\"keyWord\">\n    <h3>{{ keyWord }}</h3>\n  </div>\n</template>\n\n<script>\nimport {customRef} from 'vue'\n\nexport default {\n  name: 'App',\n  setup: function () {\n    /*使用Vue提供的ref*/\n    /*let keyWord = ref('hello')*/\n\n    /*自定义一个ref——名为:myRef*/\n    function myRef(value) {\n      console.log('--myRef', value)\n      return customRef((track, trigger) => {\n        return {\n          get: function () {\n            console.log(`有人把myRef这个容器读取数据了,我把${value}给他了`)\n            /*通知Vue追踪value的*/\n            track()\n            return value\n          },\n          set: function (newValue) {\n            console.log(`有人把myRef这个容器读取数据改为了,${newValue}`)\n            value = newValue\n            /*通知Vue去重新解析模板*/\n            trigger()\n          }\n\n        }\n      })\n    }\n\n    /*使用程序员自定义的ref*/\n    let keyWord = myRef('hello')\n\n    return {\n      keyWord,\n      myRef\n    }\n  }\n}\n</script>\n\n\n"]},"metadata":{},"sourceType":"module"}