{"ast":null,"code":"import { ref, watch, reactive, watchEffect } from \"vue\";\nexport default {\n  name: \"Demo\",\n  setup: function () {\n    let sum = ref(0);\n    let msg = ref('Morries_wu');\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        job1: {\n          salary: 20\n        }\n      }\n    });\n    /*监视\r\n    watch(sum, (newValue, oldValue) => {\r\n          console.log('sum的值变化了', newValue, oldValue)\r\n        }, {immediate: true}\r\n    )*/\n\n    watchEffect(() => {\n      console.log('watchEffect');\n    });\n    /*返回一个对象(常用)*/\n\n    return {\n      sum,\n      msg,\n      person\n    };\n  }\n};","map":{"version":3,"mappings":"AAmBA,SAAQA,GAAR,EAAaC,KAAb,EAAoBC,QAApB,EAA6BC,WAA7B,QAA+C,KAA/C;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,KAAK,EAAE,YAAY;IACjB,IAAIC,GAAE,GAAIN,GAAG,CAAC,CAAD,CAAb;IACA,IAAIO,GAAE,GAAIP,GAAG,CAAC,YAAD,CAAb;IACA,IAAIQ,MAAK,GAAIN,QAAQ,CAAC;MACpBE,IAAI,EAAE,IADc;MAEpBK,GAAG,EAAE,EAFe;MAGpBC,GAAG,EAAE;QACHC,IAAI,EAAE;UACJC,MAAM,EAAE;QADJ;MADH;IAHe,CAAD,CAArB;IAUA;;;;;;IAMAT,WAAW,CAAC,MAAI;MACdU,OAAO,CAACC,GAAR,CAAY,aAAZ;IACD,CAFU,CAAX;IAKA;;IACA,OAAO;MACLR,GADK;MAELC,GAFK;MAGLC;IAHK,CAAP;EAKD;AAhCY,CAAf","names":["ref","watch","reactive","watchEffect","name","setup","sum","msg","person","age","job","job1","salary","console","log"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\components\\Demo.vue"],"sourcesContent":["<template>\r\n  <h2>当前求和为:{{ sum }}</h2>\r\n  <button @click=\"sum++\">点我+1</button>\r\n  <br>\r\n  <h2>当前的信息为:{{ msg }}</h2>\r\n  <button @click=\"msg+='!'\">修改信息</button>\r\n  <br>\r\n  <h2>姓名:{{ person.name }}</h2>\r\n  <h2>年龄:{{ person.age }}</h2>\r\n  <h2>薪资:{{ person.job.job1.salary }}</h2>\r\n  <button @click=\"person.name+='~'\">修改姓名</button>\r\n  <br>\r\n  <button @click=\"person.age++\">增长年龄</button>\r\n  <br>\r\n  <button @click=\"person.job.job1.salary++\">涨薪</button>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {ref, watch, reactive,watchEffect} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Demo\",\r\n  setup: function () {\r\n    let sum = ref(0)\r\n    let msg = ref('Morries_wu')\r\n    let person = reactive({\r\n      name: \"张三\",\r\n      age: 18,\r\n      job: {\r\n        job1: {\r\n          salary: 20\r\n        }\r\n      }\r\n    })\r\n\r\n    /*监视\r\n    watch(sum, (newValue, oldValue) => {\r\n          console.log('sum的值变化了', newValue, oldValue)\r\n        }, {immediate: true}\r\n    )*/\r\n\r\n    watchEffect(()=>{\r\n      console.log('watchEffect')\r\n    })\r\n\r\n\r\n    /*返回一个对象(常用)*/\r\n    return {\r\n      sum,\r\n      msg,\r\n      person\r\n    }\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}