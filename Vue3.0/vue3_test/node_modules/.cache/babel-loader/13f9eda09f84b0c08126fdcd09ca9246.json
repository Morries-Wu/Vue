 age: 18,\r\n      job: {\r\n        job1: {\r\n          salary: 20\r\n        }\r\n      }\r\n    })\r\n\r\n    function showRawPerson() {\r\n      const P=toRaw()\r\n      console.log(person)\r\n    }\r\n\r\n\r\n    /*返回一个对象(常用)*/\r\n    return {\r\n      person,\r\n      ...toRefs(person),\r\n      sum,\r\n      showRawPerson\r\n    }\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}