{"ast":null,"code":"import { reactive, toRefs, shallowReactive, shallowRef, ref } from \"vue\";\nexport default {\n  name: \"Demo\",\n  setup: function () {\n    /*\r\n    只会考虑第一层数据的响应式\r\n    let person =shallowReactive({})\r\n    */\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        job1: {\n          salary: 20\n        }\n      }\n    });\n    let x = shallowRef(0);\n    /*返回一个对象(常用)*/\n\n    return {\n      person,\n      ...toRefs(person),\n      x\n    };\n  }\n};","map":{"version":3,"mappings":"AAiBA,SAAQA,QAAR,EAAkBC,MAAlB,EAA0BC,eAA1B,EAA2CC,UAA3C,EAAuDC,GAAvD,QAAiE,KAAjE;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,KAAK,EAAE,YAAY;IACjB;;;;IAKA,IAAIC,MAAK,GAAIP,QAAQ,CAAC;MACpBK,IAAI,EAAE,IADc;MAEpBG,GAAG,EAAE,EAFe;MAGpBC,GAAG,EAAE;QACHC,IAAI,EAAE;UACJC,MAAM,EAAE;QADJ;MADH;IAHe,CAAD,CAArB;IAUA,IAAIC,IAAIT,UAAU,CAAC,CAAD,CAAlB;IAGA;;IACA,OAAO;MACLI,MADK;MAEL,GAAGN,MAAM,CAACM,MAAD,CAFJ;MAGLK;IAHK,CAAP;EAKD;AA3BY,CAAf","names":["reactive","toRefs","shallowReactive","shallowRef","ref","name","setup","person","age","job","job1","salary","x"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\components\\Demo.vue"],"sourcesContent":["<template>\r\n  <h4>当前的x是{{ x }}</h4>\r\n  <button @click=\"x++\">点我x++</button>\r\n  <br>\r\n  <h4>{{ person }}</h4>\r\n  <h2>姓名:{{ name }}</h2>\r\n  <h2>年龄:{{ age }}</h2>\r\n  <h2>薪资:{{ job.job1.salary }}</h2>\r\n  <button @click=\"name+='~'\">修改姓名</button>\r\n  <br>\r\n  <button @click=\"age++\">增长年龄</button>\r\n  <br>\r\n  <button @click=\"job.job1.salary++\">涨薪</button>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {reactive, toRefs, shallowReactive, shallowRef, ref} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Demo\",\r\n  setup: function () {\r\n    /*\r\n    只会考虑第一层数据的响应式\r\n    let person =shallowReactive({})\r\n    */\r\n\r\n    let person = reactive({\r\n      name: \"张三\",\r\n      age: 18,\r\n      job: {\r\n        job1: {\r\n          salary: 20\r\n        }\r\n      }\r\n    })\r\n\r\n    let x = shallowRef(0)\r\n\r\n\r\n    /*返回一个对象(常用)*/\r\n    return {\r\n      person,\r\n      ...toRefs(person),\r\n      x\r\n    }\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}