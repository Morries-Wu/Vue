{"ast":null,"code":"import { reactive, ref, toRefs, readonly } from \"vue\";\nexport default {\n  name: \"Demo\",\n  setup: function () {\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        job1: {\n          salary: 20\n        }\n      }\n    });\n    let sum = ref(0);\n    /*返回一个对象(常用)*/\n\n    return {\n      person,\n      ...toRefs(person),\n      sum\n    };\n  }\n};","map":{"version":3,"mappings":"AAiBA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,MAAvB,EAA+BC,QAA/B,QAA8C,KAA9C;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,KAAK,EAAE,YAAY;IAGjB,IAAIC,MAAK,GAAIN,QAAQ,CAAC;MACpBI,IAAI,EAAE,IADc;MAEpBG,GAAG,EAAE,EAFe;MAGpBC,GAAG,EAAE;QACHC,IAAI,EAAE;UACJC,MAAM,EAAE;QADJ;MADH;IAHe,CAAD,CAArB;IAUA,IAAIC,GAAE,GAAIV,GAAG,CAAC,CAAD,CAAb;IAGA;;IACA,OAAO;MACLK,MADK;MAEL,GAAGJ,MAAM,CAACI,MAAD,CAFJ;MAGLK;IAHK,CAAP;EAKD;AAxBY,CAAf","names":["reactive","ref","toRefs","readonly","name","setup","person","age","job","job1","salary","sum"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\components\\Demo.vue"],"sourcesContent":["<template>\r\n  <h4>当前求和为{{ sum }}</h4>\r\n  <button @click=\"sum++\">点我sum++</button>\r\n  <br>\r\n  <h4>{{ person }}</h4>\r\n  <h2>姓名:{{ name }}</h2>\r\n  <h2>年龄:{{ age }}</h2>\r\n  <h2>薪资:{{ job.job1.salary }}</h2>\r\n  <button @click=\"name+='~'\">修改姓名</button>\r\n  <br>\r\n  <button @click=\"age++\">增长年龄</button>\r\n  <br>\r\n  <button @click=\"job.job1.salary++\">涨薪</button>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {reactive, ref, toRefs, readonly} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Demo\",\r\n  setup: function () {\r\n\r\n\r\n    let person = reactive({\r\n      name: \"张三\",\r\n      age: 18,\r\n      job: {\r\n        job1: {\r\n          salary: 20\r\n        }\r\n      }\r\n    })\r\n\r\n    let sum = ref(0)\r\n\r\n\r\n    /*返回一个对象(常用)*/\r\n    return {\r\n      person,\r\n      ...toRefs(person),\r\n      sum\r\n    }\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}