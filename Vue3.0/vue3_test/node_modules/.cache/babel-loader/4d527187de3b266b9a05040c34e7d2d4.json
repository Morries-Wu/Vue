{"ast":null,"code":"import { reactive, computed } from \"vue\";\nexport default {\n  name: \"Demo\",\n  setup: function () {\n    let person = reactive({\n      firstName: '张',\n      lastName: '三'\n    });\n    /*计算属性——简写(没有考虑计算属性被修改的情况)\r\n    person.fullName = computed(() => {\r\n      return person.firstName + '-' + person.lastName\r\n    })\r\n    */\n\n    /*计算属性——完整写法(考虑读和写)*/\n\n    person.fullName = computed({\n      get: function () {\n        return person.firstName + '-' + person.lastName;\n      },\n      set: function (value) {\n        const nameArr = value.split('-');\n        person.firstName = nameArr[0];\n        person.lastName = nameArr[1];\n      }\n    });\n    /*返回一个对象(常用)*/\n\n    return {\n      person,\n      fullName\n    };\n  }\n};","map":{"version":3,"mappings":"AAUA,SAAQA,QAAR,EAAkBC,QAAlB,QAAiC,KAAjC;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,KAAK,EAAE,YAAY;IACjB,IAAIC,MAAK,GAAIJ,QAAQ,CAAC;MACpBK,SAAS,EAAE,GADS;MAEpBC,QAAQ,EAAE;IAFU,CAAD,CAArB;IAIA;;;;;;IAMA;;IACAF,MAAM,CAACG,QAAP,GAAkBN,QAAQ,CAAC;MACzBO,GAAG,EAAE,YAAY;QACf,OAAOJ,MAAM,CAACC,SAAP,GAAmB,GAAnB,GAAyBD,MAAM,CAACE,QAAvC;MACD,CAHwB;MAIzBG,GAAG,EAAE,UAAUC,KAAV,EAAiB;QACpB,MAAMC,OAAM,GAAID,KAAK,CAACE,KAAN,CAAY,GAAZ,CAAhB;QACAR,MAAM,CAACC,SAAP,GAAmBM,OAAO,CAAC,CAAD,CAA1B;QACAP,MAAM,CAACE,QAAP,GAAkBK,OAAO,CAAC,CAAD,CAAzB;MACF;IARyB,CAAD,CAA1B;IAYA;;IACA,OAAO;MACLP,MADK;MAELG;IAFK,CAAP;EAID;AA/BY,CAAf","names":["reactive","computed","name","setup","person","firstName","lastName","fullName","get","set","value","nameArr","split"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\components\\Demo.vue"],"sourcesContent":["<template>\r\n  <h1>一个人的信息</h1>\r\n  姓: <input type=\"text\" v-model=\"person.firstName\">\r\n  <br>\r\n  名: <input type=\"text\" v-model=\"person.lastName \">\r\n  <br>\r\n  <span>全名: <input type=\"text\" v-model=\"person.fullName\"></span>\r\n</template>\r\n\r\n<script>\r\nimport {reactive, computed} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Demo\",\r\n  setup: function () {\r\n    let person = reactive({\r\n      firstName: '张',\r\n      lastName: '三',\r\n    })\r\n    /*计算属性——简写(没有考虑计算属性被修改的情况)\r\n    person.fullName = computed(() => {\r\n      return person.firstName + '-' + person.lastName\r\n    })\r\n    */\r\n\r\n    /*计算属性——完整写法(考虑读和写)*/\r\n    person.fullName = computed({\r\n      get: function () {\r\n        return person.firstName + '-' + person.lastName\r\n      },\r\n      set: function (value) {\r\n        const nameArr = value.split('-')\r\n        person.firstName = nameArr[0]\r\n        person.lastName = nameArr[1]\r\n      }\r\n    })\r\n\r\n\r\n    /*返回一个对象(常用)*/\r\n    return {\r\n      person,\r\n      fullName\r\n    }\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}