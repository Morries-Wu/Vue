{"ast":null,"code":"import { h } from 'vue';\nexport default {\n  name: 'App',\n\n  /*此处只是测试一个setup,暂时不考虑响应式问题*/\n  setup: function () {\n    /*数据*/\n    let name = 'Morries_Wu';\n    let age = 18;\n    /*方法*/\n\n    function sayHello() {\n      alert(`我叫${name},我${age}岁了,你好啊!`);\n    }\n\n    function Test3() {\n      console.log(this.name);\n      console.log(this.age);\n      console.log(this.sex);\n      console.log(sayHello);\n      console.log(this.sayWelcome);\n    }\n    /*返回一个对象(常用)*/\n\n\n    return {\n      name,\n      age,\n      sayHello,\n      Test3\n    };\n    /*返回一个函数(渲染函数)*/\n\n    return () => h('h1', '尚硅谷');\n  }\n};","map":{"version":3,"mappings":"AAQA,SAAQA,CAAR,QAAgB,KAAhB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;;EAGb;EACAC,KAAK,EAAE,YAAY;IACjB;IACA,IAAID,IAAG,GAAI,YAAX;IACA,IAAIE,GAAE,GAAI,EAAV;IAEA;;IACA,SAASC,QAAT,GAAoB;MAClBC,KAAK,CAAE,KAAIJ,IAAK,KAAIE,GAAI,SAAnB,CAAL;IACF;;IAEA,SAASG,KAAT,GAAiB;MACfC,OAAO,CAACC,GAAR,CAAY,KAAKP,IAAjB;MACAM,OAAO,CAACC,GAAR,CAAY,KAAKL,GAAjB;MACAI,OAAO,CAACC,GAAR,CAAY,KAAKC,GAAjB;MACAF,OAAO,CAACC,GAAR,CAAYJ,QAAZ;MACAG,OAAO,CAACC,GAAR,CAAY,KAAKE,UAAjB;IACF;IAEA;;;IACA,OAAO;MACLT,IADK;MAELE,GAFK;MAGLC,QAHK;MAILE;IAJK,CAAP;IAOA;;IACA,OAAO,MAAMN,CAAC,CAAC,IAAD,EAAO,KAAP,CAAd;EAEF;AAjCa,CAAf","names":["h","name","setup","age","sayHello","alert","Test3","console","log","sex","sayWelcome"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\App.vue"],"sourcesContent":["<template>\n  <h1>一个人的信息</h1>\n  <h2>姓名:{{ name }}</h2>\n  <h2>年龄:{{ age }}</h2>\n\n</template>\n\n<script>\nimport {h} from 'vue'\n\nexport default {\n  name: 'App',\n  \n  /*此处只是测试一个setup,暂时不考虑响应式问题*/\n  setup: function () {\n    /*数据*/\n    let name = 'Morries_Wu'\n    let age = 18\n\n    /*方法*/\n    function sayHello() {\n      alert(`我叫${name},我${age}岁了,你好啊!`)\n    }\n\n    function Test3() {\n      console.log(this.name)\n      console.log(this.age)\n      console.log(this.sex)\n      console.log(sayHello)\n      console.log(this.sayWelcome)\n    }\n\n    /*返回一个对象(常用)*/\n    return {\n      name,\n      age,\n      sayHello,\n      Test3\n    }\n\n    /*返回一个函数(渲染函数)*/\n    return () => h('h1', '尚硅谷')\n\n  }\n}\n</script>\n\n\n"]},"metadata":{},"sourceType":"module"}