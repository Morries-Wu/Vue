{"ast":null,"code":"import { reactive, ref, toRefs, toRaw } from \"vue\";\nexport default {\n  name: \"Demo\",\n  setup: function () {\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        job1: {\n          salary: 20\n        }\n      }\n    });\n    let sum = ref(0);\n\n    function showRawPerson() {\n      const P = toRaw(person);\n      P.age++;\n      console.log(P);\n    }\n\n    function addCar() {\n      let car = {\n        name: '奔驰',\n        price: '40w'\n      };\n      person.car = car;\n    }\n    /*返回一个对象(常用)*/\n\n\n    return {\n      person,\n      ...toRefs(person),\n      sum,\n      showRawPerson,\n      addCar\n    };\n  }\n};","map":{"version":3,"mappings":"AAmBA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,MAAvB,EAA+BC,KAA/B,QAA2C,KAA3C;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,KAAK,EAAE,YAAY;IAGjB,IAAIC,MAAK,GAAIN,QAAQ,CAAC;MACpBI,IAAI,EAAE,IADc;MAEpBG,GAAG,EAAE,EAFe;MAGpBC,GAAG,EAAE;QACHC,IAAI,EAAE;UACJC,MAAM,EAAE;QADJ;MADH;IAHe,CAAD,CAArB;IAUA,IAAIC,GAAE,GAAIV,GAAG,CAAC,CAAD,CAAb;;IAEA,SAASW,aAAT,GAAyB;MACvB,MAAMC,IAAIV,KAAK,CAACG,MAAD,CAAf;MACAO,CAAC,CAACN,GAAF;MACAO,OAAO,CAACC,GAAR,CAAYF,CAAZ;IACF;;IAEA,SAASG,MAAT,GAAkB;MAChB,IAAIC,GAAE,GAAI;QACRb,IAAI,EAAE,IADE;QAERc,KAAK,EAAE;MAFC,CAAV;MAIAZ,MAAM,CAACW,GAAP,GAAaA,GAAb;IACF;IAGA;;;IACA,OAAO;MACLX,MADK;MAEL,GAAGJ,MAAM,CAACI,MAAD,CAFJ;MAGLK,GAHK;MAILC,aAJK;MAKLI;IALK,CAAP;EAOD;AAxCY,CAAf","names":["reactive","ref","toRefs","toRaw","name","setup","person","age","job","job1","salary","sum","showRawPerson","P","console","log","addCar","car","price"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\components\\Demo.vue"],"sourcesContent":["<template>\r\n  <h4>当前求和为{{ sum }}</h4>\r\n  <button @click=\"sum++\">点我sum++</button>\r\n  <br>\r\n  <h4>{{ person }}</h4>\r\n  <h2>姓名:{{ name }}</h2>\r\n  <h2>年龄:{{ age }}</h2>\r\n  <h2>薪资:{{ job.job1.salary }}</h2>\r\n  <button @click=\"name+='~'\">修改姓名</button>\r\n  <br>\r\n  <button @click=\"age++\">增长年龄</button>\r\n  <br>\r\n  <button @click=\"job.job1.salary++\">涨薪</button>\r\n  <br>\r\n  <button @click=\"showRawPerson\">输出最原始的person</button>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {reactive, ref, toRefs, toRaw} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Demo\",\r\n  setup: function () {\r\n\r\n\r\n    let person = reactive({\r\n      name: \"张三\",\r\n      age: 18,\r\n      job: {\r\n        job1: {\r\n          salary: 20\r\n        }\r\n      }\r\n    })\r\n\r\n    let sum = ref(0)\r\n\r\n    function showRawPerson() {\r\n      const P = toRaw(person)\r\n      P.age++\r\n      console.log(P)\r\n    }\r\n\r\n    function addCar() {\r\n      let car = {\r\n        name: '奔驰',\r\n        price: '40w'\r\n      }\r\n      person.car = car\r\n    }\r\n\r\n\r\n    /*返回一个对象(常用)*/\r\n    return {\r\n      person,\r\n      ...toRefs(person),\r\n      sum,\r\n      showRawPerson,\r\n      addCar\r\n    }\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}