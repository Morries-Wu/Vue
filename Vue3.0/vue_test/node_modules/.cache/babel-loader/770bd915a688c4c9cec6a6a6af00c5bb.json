ncrementOdd: function () {\r\n      if (this.sum % 2) {\r\n        this.sum = this.sum + this.num\r\n      }\r\n\r\n    },\r\n    incrementWait: function () {\r\n      setTimeout(() => {\r\n        this.sum = this.sum + this.num\r\n      }, 500)\r\n\r\n    }\r\n\r\n  }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\nbutton {\r\n  margin-left: 5px;\r\n}\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}