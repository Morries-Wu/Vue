{"ast":null,"code":"import { reactive } from \"vue\";\nexport default {\n  name: \"Demo\",\n  setup: function () {\n    /*详单与Vue2中的$attrs*/\n    console.log(props, context);\n    /*详单与Vue2中的$attrs*/\n\n    console.log(props, context.attrs);\n    /*触发自定义事件的*/\n\n    console.log(props, context.emit);\n    /*插槽*/\n\n    console.log(props, context.slots);\n    let person = reactive({\n      name: 'Morries_Wu',\n      age: 18\n    });\n    /*方法*/\n\n    function test() {\n      context.emit('hello', 666);\n    }\n    /*返回一个对象(常用)*/\n\n\n    return {\n      person,\n      test\n    };\n  },\n\n  beforeCreate() {\n    console.log('---beforeCreate---');\n  }\n\n};","map":{"version":3,"mappings":"AAQA,SAAQA,QAAR,QAAuB,KAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAGbC,KAAK,EAAE,YAAY;IACjB;IACAC,OAAO,CAACC,GAAR,CAAYC,KAAZ,EAAmBC,OAAnB;IACA;;IACAH,OAAO,CAACC,GAAR,CAAYC,KAAZ,EAAmBC,OAAO,CAACC,KAA3B;IACA;;IACAJ,OAAO,CAACC,GAAR,CAAYC,KAAZ,EAAmBC,OAAO,CAACE,IAA3B;IACA;;IACAL,OAAO,CAACC,GAAR,CAAYC,KAAZ,EAAmBC,OAAO,CAACG,KAA3B;IAEA,IAAIC,MAAK,GAAIV,QAAQ,CAAC;MACpBC,IAAI,EAAE,YADc;MAEpBU,GAAG,EAAE;IAFe,CAAD,CAArB;IAKA;;IACA,SAASC,IAAT,GAAgB;MACdN,OAAO,CAACE,IAAR,CAAa,OAAb,EAAsB,GAAtB;IACF;IAGA;;;IACA,OAAO;MACLE,MADK;MAELE;IAFK,CAAP;EAKD,CA9BY;;EA+BbC,YAAY,GAAG;IACbV,OAAO,CAACC,GAAR,CAAY,oBAAZ;EACD;;AAjCY,CAAf","names":["reactive","name","setup","console","log","props","context","attrs","emit","slots","person","age","test","beforeCreate"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\components\\Demo.vue"],"sourcesContent":["<template>\r\n  <h1>一个人的信息</h1>\r\n  <h2>姓名:{{ person.name }}</h2>\r\n  <h2>年龄:{{ person.age }}</h2>\r\n  <button @click=\"test\">测试触发一下Demo组件的Hello事件</button>\r\n</template>\r\n\r\n<script>\r\nimport {reactive} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Demo\",\r\n\r\n  setup: function () {\r\n    /*详单与Vue2中的$attrs*/\r\n    console.log(props, context)\r\n    /*详单与Vue2中的$attrs*/\r\n    console.log(props, context.attrs)\r\n    /*触发自定义事件的*/\r\n    console.log(props, context.emit)\r\n    /*插槽*/\r\n    console.log(props, context.slots)\r\n\r\n    let person = reactive({\r\n      name: 'Morries_Wu',\r\n      age: 18,\r\n    })\r\n\r\n    /*方法*/\r\n    function test() {\r\n      context.emit('hello', 666)\r\n    }\r\n\r\n\r\n    /*返回一个对象(常用)*/\r\n    return {\r\n      person,\r\n      test\r\n    }\r\n\r\n  },\r\n  beforeCreate() {\r\n    console.log('---beforeCreate---')\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}