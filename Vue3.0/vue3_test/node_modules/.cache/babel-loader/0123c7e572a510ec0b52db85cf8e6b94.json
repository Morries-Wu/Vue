{"ast":null,"code":"import { ref } from 'vue';\nexport default {\n  name: 'App',\n  setup: function () {\n    /*数据*/\n    let name = ref('Morries_Wu');\n    let age = ref(18);\n    let job = ref({\n      type: '后端工程师',\n      salary: '30k'\n    });\n\n    function changeInfo() {\n      name.value = '老六';\n      age.value = 20;\n      job.value.type = '算法工程师';\n      job.value.salary = '50k';\n      console.log(this);\n    }\n    /*返回一个对象(常用)*/\n\n\n    return {\n      name,\n      age,\n      job,\n      changeInfo\n    };\n  }\n};","map":{"version":3,"mappings":"AAcA,SAAQA,GAAR,QAAkB,KAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,KAAK,EAAE,YAAY;IACjB;IACA,IAAID,IAAG,GAAID,GAAG,CAAC,YAAD,CAAd;IACA,IAAIG,GAAE,GAAIH,GAAG,CAAC,EAAD,CAAb;IACA,IAAII,GAAG,GAACJ,GAAG,CAAC;MACVK,IAAI,EAAC,OADK;MAEVC,MAAM,EAAC;IAFG,CAAD,CAAX;;IAKA,SAASC,UAAT,GAAsB;MACpBN,IAAI,CAACO,KAAL,GAAa,IAAb;MACAL,GAAG,CAACK,KAAJ,GAAY,EAAZ;MACAJ,GAAG,CAACI,KAAJ,CAAUH,IAAV,GAAe,OAAf;MACAD,GAAG,CAACI,KAAJ,CAAUF,MAAV,GAAiB,KAAjB;MACAG,OAAO,CAACC,GAAR,CAAY,IAAZ;IACF;IAGA;;;IACA,OAAO;MACLT,IADK;MAELE,GAFK;MAGLC,GAHK;MAILG;IAJK,CAAP;EAOF;AA5Ba,CAAf","names":["ref","name","setup","age","job","type","salary","changeInfo","value","console","log"],"sourceRoot":"","sources":["F:\\Multifunctionapplication\\Advanced Files\\IntelliJ IDEA Project\\Vue\\Vue3.0\\vue3_test\\src\\App.vue"],"sourcesContent":["<template>\n  <div align=\"center\">\n    <h1>一个人的信息</h1>\n    <h2>姓名:{{ name }}</h2>\n    <h2>年龄:{{ age }}</h2>\n    <h2>工作种类:{{job.type}}</h2>\n    <h2>工作薪水:{{job.salary}}</h2>\n    <button @click=\"changeInfo\">修改人的信息</button>\n  </div>\n\n\n</template>\n\n<script>\nimport {ref} from 'vue'\n\nexport default {\n  name: 'App',\n  setup: function () {\n    /*数据*/\n    let name = ref('Morries_Wu')\n    let age = ref(18)\n    let job=ref({\n      type:'后端工程师',\n      salary:'30k'\n    })\n\n    function changeInfo() {\n      name.value = '老六'\n      age.value = 20\n      job.value.type='算法工程师'\n      job.value.salary='50k'\